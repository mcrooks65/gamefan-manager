<h1>Games Index</h1>
<table class="table">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>
      <td><strong>Title</strong></td>
      <td><strong>Genre</strong></td>
      <td><strong>Price</strong></td>
      <td><strong>Developer</strong></td>
    </tr>
    <% @games.each do |games| %>
      <tr>
        <td><%= games.title %></td>
        <td><%= games.genre %></td>
        <td><%= games.price %></td>
        <td><%= games.developer.name %></td>
        <% end %>
      </tr>
  </tbody>
</table>
